{
  "source": "lib",
  "destination": "doc/api",
  "excludes": ["index\\.js$"],
  "plugins": [
    {"name": "esdoc-node"},
    {"name": "esdoc-standard-plugin", "option": {
      "accessor": {"access": ["public", "protected"]},
      "manual": {"files": ["CHANGELOG.md"]},
      "test": {"source": "test"},
      "brand": {
        "description": "Find the instances of an executable in the system path, implemented in JavaScript.",
        "logo": "doc/img/favicon.ico",
        "repository": "https://github.com/cedx/which.js",
        "title": "Which for JS"
      }
    }}
  ]
}

{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Program",
			"preLaunchTask": "Build",
			"request": "launch",
			"type": "node",
			"program": "${workspaceFolder}/bin/Which.js",
			"args": ["${input:command}"],
			"env": {"NODE_ENV": "development"}
		},
		{
			"name": "Tests",
			"request": "launch",
			"type": "node",
			"program": "${workspaceFolder}/node_modules/gulp/bin/gulp.js",
			"args": ["test"]
		}
	],
	"inputs": [
		{
			"description": "The name of the executable to find.",
			"id": "command",
			"type": "promptString"
		}
	]
}

{
	"$schema": "vscode://schemas/launch",
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Program",
			"request": "launch",
			"type": "node",
			"console": "externalTerminal",
			"cwd": "${workspaceFolder}",
			"program": "${workspaceFolder}/bin/Which.js",
			"skipFiles": ["<node_internals>/**"]
		}
	]
}
